{
    "id": "catppuccin-mocha",
    "name": "Catppuccin Mocha",
    "type": "dark",
    "version": "1.0.0",
    "description": "Catppuccin Mocha - soothing pastel dark theme",
    "colors": {
        "background": {
            "primary": "#1e1e2e",
            "secondary": "#181825",
            "card": "rgba(49, 50, 68, 0.85)",
            "cardHover": "rgba(69, 71, 90, 0.9)"
        },
        "accent": {
            "unreadBadge": "#cba6f7",
            "readBadge": "#a6e3a1",
            "link": "#89b4fa",
            "hoverAccent": "#b4befe"
        },
        "text": {
            "primary": "#cdd6f4",
            "secondary": "#bac2de",
            "dimmed": "#6c7086",
            "status": "#89b4fa"
        },
        "border": {
            "card": "rgba(88, 91, 112, 0.6)",
            "cardHover": "rgba(137, 180, 250, 0.4)",
            "divider": "rgba(88, 91, 112, 0.4)"
        }
    },
    "typography": {
        "fontFamily": "'Fira Code', 'JetBrains Mono', 'Source Code Pro', Menlo, monospace",
        "sizes": {
            "sectionHeader": "11px",
            "feedItem": "13px",
            "badge": "11px",
            "articleTitle": "16px",
            "articleSubtitle": "14px",
            "preview": "13px",
            "metadata": "11px"
        },
        "weights": {
            "regular": 400,
            "semibold": 600,
            "bold": 700
        },
        "lineHeights": {
            "compact": 1.4,
            "normal": 1.5,
            "relaxed": 1.6
        }
    },
    "spacing": {
        "cardMargin": "12px",
        "cardPadding": "16px",
        "sectionSpacing": "16px",
        "itemLineHeight": 1.6,
        "gapSmall": "8px",
        "gapMedium": "12px",
        "gapLarge": "16px"
    },
    "borderRadius": {
        "card": "10px",
        "badge": "4px",
        "thumbnail": "6px"
    },
    "shadows": {
        "card": "0 4px 16px rgba(0, 0, 0, 0.25)",
        "cardHover": "0 8px 24px rgba(0, 0, 0, 0.35)",
        "thumbnail": "0 2px 8px rgba(0, 0, 0, 0.3)"
    },
    "effects": {
        "glassBlur": "blur(8px)",
        "backdropFilter": "blur(8px)",
        "transitionDuration": "150ms",
        "transitionEasing": "ease-out"
    }
}