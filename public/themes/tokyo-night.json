{
    "id": "tokyo-night",
    "name": "Tokyo Night",
    "type": "dark",
    "version": "1.0.0",
    "description": "Inspired by the Tokyo Night color scheme",
    "colors": {
        "background": {
            "primary": "#1a1b26",
            "secondary": "#16161e",
            "card": "rgba(36, 40, 59, 0.85)",
            "cardHover": "rgba(41, 46, 66, 0.9)"
        },
        "accent": {
            "unreadBadge": "#bb9af7",
            "unreadBadgeText": "#1a1b26",
            "readBadge": "#9ece6a",
            "link": "#7aa2f7",
            "hoverAccent": "#89b4fa",
            "feedsUnreadText": "#bb9af7"
        },
        "text": {
            "primary": "#c0caf5",
            "secondary": "#a9b1d6",
            "dimmed": "#565f89",
            "status": "#7aa2f7"
        },
        "border": {
            "card": "rgba(59, 66, 97, 0.6)",
            "cardHover": "rgba(122, 162, 247, 0.4)",
            "divider": "rgba(59, 66, 97, 0.4)"
        }
    },
    "typography": {
        "fontFamily": "'Fira Code', 'JetBrains Mono', 'Source Code Pro', Menlo, monospace",
        "sizes": {
            "sectionHeader": "11px",
            "feedItem": "13px",
            "badge": "11px",
            "articleTitle": "16px",
            "articleSubtitle": "14px",
            "preview": "13px",
            "metadata": "11px"
        },
        "weights": {
            "regular": 400,
            "semibold": 600,
            "bold": 700
        },
        "lineHeights": {
            "compact": 1.4,
            "normal": 1.5,
            "relaxed": 1.6
        }
    },
    "spacing": {
        "cardMargin": "12px",
        "cardPadding": "16px",
        "sectionSpacing": "16px",
        "itemLineHeight": 1.6,
        "gapSmall": "8px",
        "gapMedium": "12px",
        "gapLarge": "16px"
    },
    "borderRadius": {
        "card": "10px",
        "badge": "4px",
        "thumbnail": "6px"
    },
    "shadows": {
        "card": "0 4px 16px rgba(0, 0, 0, 0.25)",
        "cardHover": "0 8px 24px rgba(0, 0, 0, 0.35)",
        "thumbnail": "0 2px 8px rgba(0, 0, 0, 0.3)"
    },
    "effects": {
        "glassBlur": "blur(8px)",
        "backdropFilter": "blur(8px)",
        "transitionDuration": "150ms",
        "transitionEasing": "ease-out"
    }
}